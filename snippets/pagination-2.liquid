{%- if paginate.parts.size > 0 -%}
  <div class="pagination-wrapper">
    <nav class="pagination_new" role="navigation" aria-label="{{ 'general.pagination.label' | t }}">
      <ul class="pagination__list list-unstyled" role="list">
        
          <li>
            <a
              href="{{ paginate.previous.url }}{{ anchor }}"
              class="button button-prev-text button--outline pagination__item pagination__item--next pagination__item-arrow"
              aria-label="Previous"
              {% unless paginate.previous %}disabled{% endunless %}
            >
              Prev
            </a>
            <a
              href="{{ paginate.previous.url }}{{ anchor }}"
              class="button button-left-arrow-icon button--outline pagination__item pagination__item--next pagination__item-arrow"
              aria-label="Previous"
              {% unless paginate.previous %}disabled{% endunless %}
            >
              <svg xmlns="http://www.w3.org/2000/svg" width="12" height="20" viewBox="0 0 12 20" fill="none">
                <path d="M10 18L2 10L10 2" stroke="#111111" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
            </a>
          </li>
        
<ul class="pagination_sub_list">
        {%- for part in paginate.parts -%}
          <li>
            {%- if part.is_link -%}
              <a
                href="{{ part.url }}{{ anchor }}"
                class="pagination__item"
                aria-label="page-{{ part.title }}"
              >
                {{- part.title -}}
              </a>
            {%- else -%}
              {%- if part.title == paginate.current_page -%}
                <a
                  role="link"
                  aria-disabled="true"
                  class="button button--outline pagination__item pagination__item--current"
                  aria-current="page"
                  aria-label="page-{{ part.title }}"
                >
                  {{- part.title -}}
                </a>
              {%- else -%}
                <span class="pagination__item">{{ part.title }}</span>
              {%- endif -%}
            {%- endif -%}
          </li>
        {%- endfor -%}
</ul>
        
          <li>
            <a
              href="{{ paginate.next.url }}{{ anchor }}"
              class="button button-next-text button--outline pagination__item pagination__item--prev pagination__item-arrow motion-reduce"
              aria-label="Next"
              {% unless paginate.next %}disabled{% endunless %}
              
            >
              Next
            </a>
            <a
              href="{{ paginate.next.url }}{{ anchor }}"
              class="button button-right-arrow-icon button--outline pagination__item pagination__item--prev pagination__item-arrow motion-reduce"
              aria-label="Next"
              {% unless paginate.next %}disabled{% endunless %}
              
            >
              <svg xmlns="http://www.w3.org/2000/svg" width="12" height="20" viewBox="0 0 12 20" fill="none">
                  <path d="M2 18L10 10L2 2" stroke="#111111" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
            </a>
          </li>
      </ul>
    </nav>
  </div>
{%- endif -%}
